[color]
	ui = true
[push]
	default = simple
[user]
	name = Di Huynh
  email = dihuynh@cs.utexas.edu

[filter "media"]
	clean = git-media-clean %f
[color]
    ui = auto
    interactive = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = green
    changed = magenta
    untracked = bold yellow
